<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vardhan.wir">

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-feature android:name="android.hardware.wifi" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".MyFirebaseInstanceIdService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
        <service android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <activity android:name=".MainActivity" />
        <activity android:name=".Signup" />
        <activity android:name=".Profile" />
        <activity android:name=".inst" />
        <activity android:name=".startgame" />
        <activity android:name=".options" />
        <activity android:name=".Questions" />
        <activity android:name=".inputque" />
        <activity android:name=".endgame" />
        <activity android:name=".CheckUser" />
        <activity android:name=".challenge" />
        <activity android:name=".acceptreq" />
        <activity android:name=".acceptend" />
        <activity android:name=".multiplay" />
        <activity android:name=".hostque" />
        <activity android:name=".joinque" />
        <activity android:name=".searchpro" />
        <activity android:name=".hostuser" />
        <activity android:name=".leaderboard" />
        <activity android:name=".CodeMulti" />
        <activity android:name=".multicque" />
        <activity android:name=".multicscore" />
        <activity android:name=".Methods" />
        <activity android:name=".ch"></activity>
    </application>

</manifest>
